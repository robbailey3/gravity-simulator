{
  "extends": ["tslint-config-airbnb", "tslint:latest"],
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "member-ordering": [true, { "order": "fields-first" }],
    "completed-docs": [
      true,
      {
        "methods": { "privacies": ["public", "protected"] },
        "properties": true
      }
    ]
  }
}
